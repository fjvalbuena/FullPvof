                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @PeriodoPVO varchar(6), @PeriodoEstI date, @PeriodoEstF date

SELECT 
  @PeriodoPVO  = Parametros.PeriodoAAMM
, @PeriodoEstI = Parametros.PeriodoEstI
, @PeriodoEstF = Parametros.PeriodoEstF
FROM lukerVen.PVOF.Parametros Parametros
WHERE (Parametros.EstadoProceso='1')

SELECT Parametros.PeriodoAAMM
, CategoriaMarcaCanal.Cod_Negocio
, Facturas.Cod_OrgVentas
, CASE Clientes.Cod_Zona_Ventas WHEN '' THEN CASE isnull(EquivalenciaJerarquiaOrph.Cod_Cliente, '') WHEN '' THEN Facturas.Cod_Cliente ELSE isnull(EquivalenciaJerarquiaOrph.Cod_Cliente, '') END ELSE Facturas.Cod_Cliente END AS Cod_Cliente
, Productos.JerarquiaProducto
, Facturas.Cod_SKU
, max(Facturas.FechaFactura) FechaFactura
, sum(Facturas.Kilos) as Kilos
, sum(Facturas.ValorNeto) as ValorNeto
FROM        SD.FacturasW AS Facturas INNER JOIN
                  SD.CanalDistribucion ON Facturas.Cod_Canal = SD.CanalDistribucion.Cod_Canal INNER JOIN
                  SD.Productos ON Facturas.Cod_SKU = SD.Productos.Cod_SKU INNER JOIN
                  SD.CategoriaMarcaCanal ON SD.Productos.Aliado = SD.CategoriaMarcaCanal.Aliado AND SD.Productos.Categoria = SD.CategoriaMarcaCanal.Categoria AND SD.Productos.Marca = SD.CategoriaMarcaCanal.Marca AND 
                  Facturas.Cod_Canal = SD.CategoriaMarcaCanal.Cod_Canal AND Facturas.Cod_OrgVentas = SD.CategoriaMarcaCanal.Cod_OrgVentas INNER JOIN
                  SD.MotivosNoVenta ON Facturas.Cod_MotivoNoVenta = SD.MotivosNoVenta.Cod_Motivo LEFT OUTER JOIN
                  SD.Clientes ON Facturas.Cod_Cliente = SD.Clientes.Cod_Cliente LEFT OUTER JOIN
                  SD.AliasSKU ON Facturas.Cod_SKU = SD.AliasSKU.Cod_SKU LEFT OUTER JOIN
                  SD.EquivalenciaJerarquiaOrph AS EquivalenciaJerarquiaOrph ON EquivalenciaJerarquiaOrph.Cod_Canal = Facturas.Cod_Canal AND EquivalenciaJerarquiaOrph.Cod_GCliente = Facturas.Cod_GCliente AND 
                  Facturas.SalesOffice = EquivalenciaJerarquiaOrph.Cod_Oficina_Ventas
				  inner join lukerVen.SD.TipoDocumento TipoDocumento  on TipoDocumento.Cod_TipoFactura = Facturas.Cod_TipoFactura 
, lukerVen.PVOF.Parametros Parametros

where  ( (Parametros.EstadoProceso=1)
AND (year(Facturas.FechaFactura)+1&gt;=left(@PeriodoPVO,4)) 
 
)
group by 
Parametros.PeriodoAAMM
, CategoriaMarcaCanal.Cod_Negocio
, Facturas.Cod_OrgVentas
, CASE Clientes.Cod_Zona_Ventas WHEN '' THEN CASE isnull(EquivalenciaJerarquiaOrph.Cod_Cliente, '') WHEN '' THEN Facturas.Cod_Cliente ELSE isnull(EquivalenciaJerarquiaOrph.Cod_Cliente, '') END ELSE Facturas.Cod_Cliente END 
, Productos.JerarquiaProducto
, Facturas.Cod_SKU
, year(Facturas.FechaFactura)
, month(Facturas.FechaFactura)
having sum(Facturas.Kilos) &lt;&gt;0 or  sum(Facturas.ValorNeto)  &lt;&gt;0</property>

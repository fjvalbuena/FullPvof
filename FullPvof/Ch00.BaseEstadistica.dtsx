      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C506684D-3787-4769-AEE6-43F1D50F46AF}"
          SQLTask:SqlStatementSource="select(getdate()), 'ini'&#xA;declare @PeriodoPVO varchar(6), @PeriodoEstI date, @PeriodoEstF date&#xA;SELECT &#xA;  @PeriodoPVO  = Parametros.PeriodoAAMM&#xA;, @PeriodoEstI = Parametros.PeriodoEstI&#xA;, @PeriodoEstF = Parametros.PeriodoEstF&#xA;FROM lukerVen.PVOF.Parametros Parametros&#xA;WHERE (Parametros.EstadoProceso='1')&#xA;&#xA;&#xA;insert into lukerVen.hpvo.BaseEstadisticaHistoria&#xA;SELECT BaseEstadisticaHistoria.Periodo, BaseEstadisticaHistoria.Cod_Negocio, BaseEstadisticaHistoria.Cod_OrgVentas&#xA;, xppt_cambiodistriatat.Cod_Cliente_Nueva_Razon, BaseEstadisticaHistoria.JerarquiaProducto&#xA;, BaseEstadisticaHistoria.Cod_SKU, BaseEstadisticaHistoria.FechaFactura&#xA;, (BaseEstadisticaHistoria.Kilos* xppt_cambiodistriatat.Porcentaje)&#xA;, (BaseEstadisticaHistoria.ValorNeto* xppt_cambiodistriatat.Porcentaje)&#xA;, 'DaT'&#xA;FROM lukerVen.hpvo.BaseEstadisticaHistoria BaseEstadisticaHistoria&#xA;inner join lukerVen.tmp.xppt_cambiodistriatat xppt_cambiodistriatat&#xA;on xppt_cambiodistriatat.Cod_Cliente = BaseEstadisticaHistoria.Cod_Cliente&#xA;&#xA;select(getdate()),'fin DaT'&#xA;&#xA;&#xA;&#xA;update BaseEstadisticaHistoria set&#xA;BaseEstadisticaHistoria.Periodo= 'toTAT'&#xA;FROM lukerVen.hpvo.BaseEstadisticaHistoria BaseEstadisticaHistoria, lukerVen.tmp.xppt_cambiodistriatat xppt_cambiodistriatat&#xA;WHERE xppt_cambiodistriatat.Cod_Cliente = BaseEstadisticaHistoria.Cod_Cliente&#xA;select(getdate()), 'toTAT'&#xA;&#xA;-- Espejo&#xA;update BaseEstadisticaHistoria set&#xA;BaseEstadisticaHistoria.Cod_Cliente= ClientesEspejo.Cod_Cliente_Espejo&#xA;, origen='Esp'+left(origen,1)&#xA;FROM lukerVen.hpvo.BaseEstadisticaHistoria BaseEstadisticaHistoria, lukerVen.SD.ClientesEspejo ClientesEspejo, lukerVen.SD.Productos Productos, lukerVen.dbo.xppt_portafolio_marca xppt_portafolio_marca&#xA;WHERE BaseEstadisticaHistoria.Cod_Cliente = ClientesEspejo.Cod_Cliente_Base &#xA;AND Productos.Cod_SKU = BaseEstadisticaHistoria.Cod_SKU &#xA;AND Productos.Marca = xppt_portafolio_marca.Marca &#xA;AND ClientesEspejo.Cod_Portafolio_Espejo = xppt_portafolio_marca.Portafolio&#xA;and BaseEstadisticaHistoria.Periodo= @PeriodoPVO&#xA;--and BaseEstadisticaHistoria.FechaFactura&lt;'2020-03-01'&#xA;select(getdate()), 'Esp'&#xA;&#xA;update BaseEstadisticaHistoria set&#xA;BaseEstadisticaHistoria.Cod_Cliente= xppt_cambiorazonsocial.Cod_Cliente_Nueva_Razon&#xA;, BaseEstadisticaHistoria.origen= 'Crz'&#xA;FROM lukerVen.hpvo.BaseEstadisticaHistoria BaseEstadisticaHistoria, lukerVen.tmp.xppt_cambiorazonsocial xppt_cambiorazonsocial&#xA;WHERE BaseEstadisticaHistoria.Cod_Cliente = xppt_cambiorazonsocial.Cod_Cliente" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>

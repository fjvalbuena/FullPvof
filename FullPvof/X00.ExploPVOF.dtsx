          SQLTask:Connection="{8327733D-23AA-4BD6-9E10-ACF2EBCB2FF9}"
          SQLTask:SqlStatementSource="declare @PeriodoPVO varchar(6), @MesCalc_I int, @MesCalc_F int, @CorteMes1 date, @KGMes varchar(10), @Conta int&#xA;&#xA;SELECT &#xA;  @PeriodoPVO  = Parametros.PeriodoAAMM&#xA;, @MesCalc_I = Parametros.MesCalc_I&#xA;, @MesCalc_F = Parametros.MesCalc_F&#xA;FROM lukerVen.PVOF.Parametros Parametros&#xA;WHERE (Parametros.EstadoProceso='1')&#xA;&#xA;truncate table [PVOF].[PresupuestoTRIM]&#xA;&#xA;select @Conta=1&#xA;&#xA;&#xA;while @Conta&lt;= (@MesCalc_F+1-@MesCalc_I)&#xA;begin&#xA;&#x9;select &#x9;@KGMes='Kilos'+ltrim(str(@Conta))&#xA;&#x9;&#xA;&#x9;select @CorteMes1= ( SELECT Max(Time.PK_Date) AS 'Max of PK_Date'&#xA;&#x9;&#x9;FROM lukerVen.dbo.Time Time&#xA;&#x9;&#x9;WHERE (year(PK_Date)=left(@PeriodoPVO,4)) AND (month(PK_Date)=@MesCalc_I-1+@Conta)&#xA;&#x9;)&#xA;&#x9;print(@Conta)&#xA;&#x9;print(@CorteMes1)&#xA;&#x9;print(@KGMes)&#xA;&#xA;&#xA;INSERT INTO [PVOF].[PresupuestoTRIM]&#xA;SELECT 'B2C'&#xA;, left(VolumenTrimDLC.PeriodoAAMM,4) AS 'ano'&#xA;, @CorteMes1, BaseEstadisticaBc.Cod_Cliente&#xA;, BaseEstadisticaBc.JerarquiaProducto&#xA;, BaseEstadisticaBc.Cod_SKU&#xA;&#xA;, sum(case @Conta when 1 then VolumenTrimDLC.Kilos1 when 2 then VolumenTrimDLC.Kilos2 when 3 then VolumenTrimDLC.Kilos3 else 0 end  / BaseEstadisticaBcTDLC.Kilos * BaseEstadisticaBc.Kilos)&#xA;&#xA;, 0, 0, ''&#xA;, ClientesVW.Cod_Oficina_Ventas&#xA;, BaseEstadisticaBc.Cod_Canal&#xA;, ClientesVW.Cod_GCliente&#xA;, 0, 'Exp'&#xA;, VolumenTrimDLC.Director_Canal&#xA;FROM lukerVen.PVOF.BaseEstadisticaBc BaseEstadisticaBc, lukerVen.tmp.BaseEstadisticaBcTDLC BaseEstadisticaBcTDLC, lukerVen.SD.ClientesVW ClientesVW, lukerVen.PVOF.VolumenTrimDLC VolumenTrimDLC&#xA;WHERE BaseEstadisticaBcTDLC.Categoria = VolumenTrimDLC.Categoria AND BaseEstadisticaBcTDLC.Director_Canal = VolumenTrimDLC.Director_Canal &#xA;AND BaseEstadisticaBcTDLC.Linea = VolumenTrimDLC.Linea AND BaseEstadisticaBcTDLC.Marca = VolumenTrimDLC.Marca &#xA;AND BaseEstadisticaBcTDLC.Nombre_GCliente_AD2 = VolumenTrimDLC.Nombre_GCliente_AD2 AND BaseEstadisticaBcTDLC.NombreCanalAgrupado = VolumenTrimDLC.NombreCanalAgrupado &#xA;AND VolumenTrimDLC.PeriodoAAMM = BaseEstadisticaBcTDLC.Periodo AND BaseEstadisticaBc.Categoria = BaseEstadisticaBcTDLC.Categoria &#xA;AND BaseEstadisticaBc.Director_Canal = BaseEstadisticaBcTDLC.Director_Canal AND BaseEstadisticaBc.Linea = BaseEstadisticaBcTDLC.Linea &#xA;AND BaseEstadisticaBc.Marca = BaseEstadisticaBcTDLC.Marca AND BaseEstadisticaBc.Nombre_GCliente_AD2 = BaseEstadisticaBcTDLC.Nombre_GCliente_AD2 &#xA;AND BaseEstadisticaBc.NombreCanalAgrupado = BaseEstadisticaBcTDLC.NombreCanalAgrupado AND BaseEstadisticaBc.Periodo = BaseEstadisticaBcTDLC.Periodo &#xA;AND BaseEstadisticaBc.Cod_Cliente = ClientesVW.Cod_Cliente&#xA;and VolumenTrimDLC.PeriodoAAMM=@PeriodoPVO&#xA;group by&#xA;  left(VolumenTrimDLC.PeriodoAAMM,4)  &#xA;, BaseEstadisticaBc.Cod_Cliente&#xA;, BaseEstadisticaBc.JerarquiaProducto&#xA;, BaseEstadisticaBc.Cod_SKU&#xA;, ClientesVW.Cod_Oficina_Ventas&#xA;, BaseEstadisticaBc.Cod_Canal&#xA;, ClientesVW.Cod_GCliente&#xA;, VolumenTrimDLC.Director_Canal&#xA;&#xA;&#x9;select @Conta=@Conta+1&#xA;end" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

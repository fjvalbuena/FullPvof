      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{62A48BE8-7190-4AD6-B56B-8BFFDBF9D8B2}"
          SQLTask:SqlStatementSource="&#xA;&#xA;declare @PeriodoPVO varchar(6), @MesCalc_I int, @MesCalc_F int, @CorteMes1 date, @KGMes varchar(10), @Conta int&#xA;&#xA;SELECT &#xA;  @PeriodoPVO  = Parametros.PeriodoAAMM&#xA;, @MesCalc_I = Parametros.MesCalc_I&#xA;, @MesCalc_F = Parametros.MesCalc_F&#xA;FROM lukerVen.PVOF.Parametros Parametros&#xA;WHERE (Parametros.EstadoProceso='1')&#xA;&#xA;&#xA;&#xA;--*** Sht_Director_Canal in VolumenTrimDLC_OL&#xA;&#xA;select @Conta=1&#xA;&#xA;&#xA;while @Conta&lt;= (@MesCalc_F+1-@MesCalc_I)&#xA;begin&#xA;&#x9;select &#x9;@KGMes='Kilos'+ltrim(str(@Conta))&#xA;&#x9;&#xA;&#x9;select @CorteMes1= ( SELECT Max(Time.PK_Date) AS 'Max of PK_Date'&#xA;&#x9;&#x9;FROM lukerVen.dbo.Time Time&#xA;&#x9;&#x9;WHERE (year(PK_Date)=left(@PeriodoPVO,4)) AND (month(PK_Date)=@MesCalc_I-1+@Conta)&#xA;&#x9;)&#xA;&#x9;print(@Conta)&#xA;&#x9;print(@CorteMes1)&#xA;&#x9;print(@KGMes)&#xA;&#xA;delete PresupuestoTRIM&#xA;FROM lukerVen.PVOF.PresupuestoTRIM PresupuestoTRIM&#xA;where Sht_Director_Canal  in (&#xA;&#x9;SELECT DISTINCT VolumenTrimDLC_OL.Director_Canal&#xA;&#x9;FROM lukerVen.PVOF.VolumenTrimDLC_OL VolumenTrimDLC_OL&#xA;) and FechaCorte=@CorteMes1&#xA;&#xA;&#xA;INSERT INTO [PVOF].[PresupuestoTRIM]&#xA;SELECT 'B2C'&#xA;, left(VolumenTrimDLC_OL.PeriodoAAMM,4) AS 'ano'&#xA;, @CorteMes1&#xA;, BaseEstadisticaBc.Cod_Cliente&#xA;, BaseEstadisticaBc.JerarquiaProducto&#xA;, BaseEstadisticaBc.Cod_SKU&#xA;&#xA;, sum(case @Conta when 1 then VolumenTrimDLC_OL.Kilos1 when 2 then VolumenTrimDLC_OL.Kilos2 when 3 then VolumenTrimDLC_OL.Kilos3 else 0 end  / BaseEstadisticaBcTDLC_AL.Kilos * BaseEstadisticaBc.Kilos)&#xA;&#xA;, 0, 0, '', ClientesVW.Cod_Oficina_Ventas, BaseEstadisticaBc.Cod_Canal, ClientesVW.Cod_GCliente, 0, 'Exp', VolumenTrimDLC_OL.Director_Canal&#xA;&#xA;FROM lukerVen.PVOF.BaseEstadisticaBc BaseEstadisticaBc&#xA;, lukerVen.tmp.BaseEstadisticaBcTDLC_AL BaseEstadisticaBcTDLC_AL&#xA;, lukerVen.SD.ClientesVW ClientesVW&#xA;, lukerVen.PVOF.VolumenTrimDLC_OL VolumenTrimDLC_OL&#xA;WHERE BaseEstadisticaBcTDLC_AL.Categoria = VolumenTrimDLC_OL.Categoria AND BaseEstadisticaBcTDLC_AL.Director_Canal = VolumenTrimDLC_OL.Director_Canal &#xA;AND BaseEstadisticaBcTDLC_AL.Lider_Regional = VolumenTrimDLC_OL.Lider_Regional AND BaseEstadisticaBcTDLC_AL.Linea = VolumenTrimDLC_OL.Linea AND BaseEstadisticaBcTDLC_AL.Marca = VolumenTrimDLC_OL.Marca &#xA;AND BaseEstadisticaBcTDLC_AL.Nombre_Oficina_Ventas = VolumenTrimDLC_OL.Nombre_Oficina_Ventas AND BaseEstadisticaBcTDLC_AL.NombreCanalAgrupado = VolumenTrimDLC_OL.NombreCanalAgrupado &#xA;AND VolumenTrimDLC_OL.PeriodoAAMM = BaseEstadisticaBcTDLC_AL.Periodo AND BaseEstadisticaBc.Categoria = BaseEstadisticaBcTDLC_AL.Categoria &#xA;AND BaseEstadisticaBc.Director_Canal = BaseEstadisticaBcTDLC_AL.Director_Canal AND BaseEstadisticaBc.Lider_Regional = BaseEstadisticaBcTDLC_AL.Lider_Regional AND BaseEstadisticaBc.Linea = BaseEstadisticaBcTDLC_AL.Linea &#xA;AND BaseEstadisticaBc.Marca = BaseEstadisticaBcTDLC_AL.Marca AND BaseEstadisticaBc.Nombre_Oficina_Ventas = BaseEstadisticaBcTDLC_AL.Nombre_Oficina_Ventas &#xA;AND BaseEstadisticaBc.NombreCanalAgrupado = BaseEstadisticaBcTDLC_AL.NombreCanalAgrupado AND BaseEstadisticaBc.Periodo = BaseEstadisticaBcTDLC_AL.Periodo &#xA;AND BaseEstadisticaBc.Cod_Cliente = ClientesVW.Cod_Cliente AND ((BaseEstadisticaBcTDLC_AL.Nombre_GCliente_AD2='') &#xA;AND (VolumenTrimDLC_OL.PeriodoAAMM=@PeriodoPVO))&#xA;and BaseEstadisticaBcTDLC_AL.Kilos&lt;&gt;0&#xA;group by&#xA;  left(VolumenTrimDLC_OL.PeriodoAAMM,4) &#xA;, BaseEstadisticaBc.Cod_Cliente&#xA;, BaseEstadisticaBc.JerarquiaProducto&#xA;, BaseEstadisticaBc.Cod_SKU&#xA;, ClientesVW.Cod_Oficina_Ventas, BaseEstadisticaBc.Cod_Canal, ClientesVW.Cod_GCliente&#xA;, VolumenTrimDLC_OL.Director_Canal&#xA;&#xA;&#x9;select @Conta=@Conta+1&#xA;end" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
